<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainAnchor"
            maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="400.0"
            prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.LoginController">
   <children>
      <TextField fx:id="username" layoutX="199.0" layoutY="74.0" />
      <PasswordField fx:id="password" layoutX="199.0" layoutY="155.0" />
      <Button layoutX="259.0" layoutY="222.0" mnemonicParsing="false" onAction="#authenticateUser" text="Submit" />
      <Label layoutX="265.0" layoutY="39.0" text="Username" />
      <Label layoutX="265.0" layoutY="123.0" text="Password" />
   </children>
</AnchorPane>
